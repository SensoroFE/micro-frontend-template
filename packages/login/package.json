{
  "name": "@sensoro/login",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "homepage": "/login",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "1.6.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "umi": "^3.2.17"
  },
  "devDependencies": {
    "@umijs/plugin-qiankun": "^2.9.1"
  }
}
